#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define N 100
typedef int Tab[N];

void remplirTableau(int  *t, int n)
{
    int i;
    srand(time(NULL)); //initialise le random
    for(i=0;i<n;i++)
    {
        t[i]=rand()%n; //t[i] reçoit une valeur aléatoire modulo n
    }
}

void recopie(int *t, Tab t2, int n)
{
    int i;
    for(i=0; i<n; i++)
    {
        t2[i]=t[i];
    }
}

void affichage(int *t, int n)
{
    int i;
    for(i=0;i<n;i++)
    {
        printf("%d\n",t[i]);
    }
}
